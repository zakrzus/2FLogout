{
  "manifest_version": 2,

  "name": "Google Logouter",
  "description": "Logouts from google",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "http://*/*", "https://*/*",
    "activeTab",
    "storage"
  ]
}